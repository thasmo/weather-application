{
	"type": "module",
	"private": true,
	"packageManager": "pnpm@10.28.1",
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	},
	"scripts": {
		"dev": "vite",
		"preview": "vite preview",
		"build": "vite build",
		"deploy": "wrangler deploy",
		"upload": "wrangler versions upload",
		"prepare": "concurrently --group --prefix-colors 'auto' 'pnpm:prepare:*'",
		"prepare:husky": "husky",
		"prepare:wrangler": "wrangler types",
		"lint": "concurrently --group --prefix-colors 'auto' 'pnpm:lint:*'",
		"lint:types": "vue-tsc --noEmit",
		"lint:eslint": "eslint .",
		"lint:cspell": "cspell --no-progress --no-summary --no-must-find-files .",
		"lint:knip": "knip --no-progress",
		"fix": "concurrently --group --prefix-colors 'auto' 'pnpm:fix:*'",
		"fix:eslint": "eslint --fix .",
		"fix:knip": "knip --fix --no-progress --workspace ."
	},
	"dependencies": {
		"@ark-ui/vue": "^5.14.2",
		"@iconify-json/ri": "^1.2.5",
		"@iconify-json/wi": "^1.2.1",
		"@vueuse/core": "^14.0.0",
		"openmeteo": "^1.2.0",
		"pinia": "^3.0.3",
		"pinia-plugin-persistedstate": "^4.3.0",
		"vue": "^3.5.13",
		"vue-i18n": "^11.1.6",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.21.0",
		"@cloudflare/workers-types": "^4.20260115.0",
		"@commitlint/cli": "^20.0.0",
		"@iconify/utils": "^3.0.0",
		"@somehow-digital/commitlint-config": "^1.0.0",
		"@somehow-digital/cspell-dictionary": "^2.7.0",
		"@somehow-digital/eslint-config": "^4.0.0",
		"@somehow-digital/typescript-config": "^3.0.0",
		"@tsconfig/node24": "^24.0.0",
		"@types/node": "^24.0.0",
		"@unocss/preset-web-fonts": "^66.3.1",
		"@unocss/reset": "^66.0.0",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vue/tsconfig": "^0.8.0",
		"concurrently": "^9.1.0",
		"cspell": "^9.0.0",
		"eslint": "^9.17.0",
		"husky": "^9.1.7",
		"jiti": "^2.4.2",
		"knip": "^5.40.0",
		"lint-staged": "^16.0.0",
		"typescript": "^5.7.2",
		"unocss": "^66.0.0",
		"vite": "^7.0.0",
		"vite-plugin-vue-devtools": "^8.0.0",
		"vue-tsc": "^3.0.0",
		"wrangler": "^4.59.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
